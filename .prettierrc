{
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"proseWrap": "preserve",
	"printWidth": 120,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": false,
	"bracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore"
}
